[hub]
	protocol = git
[user]
	email = {{ .email }}
	name = {{ .name }}
[core]
	editor = {{ .editor }}
[alias]
	pushf = push --force-with-lease
	wip = commit --no-verify --no-gpg-sign -m \"--wip-- [skip ci]\"
[push]
	default = upstream
[init]
	defaultBranch = main
[includeif "gitdir:~/workspace/valetservice/"]
	path = ~/workspace/valetservice/.gitconfig
